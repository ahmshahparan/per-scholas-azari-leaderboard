{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
 "env": {
  "ADMIN_USERNAME": "$ADMIN_USERNAME",
  "ADMIN_PASSWORD": "$ADMIN_PASSWORD", 
  "JWT_SECRET": "$JWT_SECRET",
  "SUPABASE_URL": "$SUPABASE_URL",
  "SUPABASE_ANON_KEY": "$SUPABASE_ANON_KEY"
},
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}

